{"table": {
	"id": "{{newRandomID}}",
	"class": "file",
	"childNodes": [
		{"tr": {
			"childNodes": [
				{"td": {
					"class":"filename",
					"childNodes": [
						{"div": {
							"class":"owner mode {{ownermode}}",
							"childNodes": [
								{"div":{"class":"read","textContent":"r"}},
								{"div":{"class":"write","textContent":"w"}},
								{"div":{"class":"execute","textContent":"x"}}	
							]
						}},
						{"div": {
							"class":"group mode {{groupmode}}",
							"childNodes": [
								{"div":{"class":"read","textContent":"r"}},
								{"div":{"class":"write","textContent":"w"}},
								{"div":{"class":"execute","textContent":"x"}}		
							]
						}},
						{"div": {
							"class":"world mode {{worldmode}}",
							"childNodes": [
								{"div":{"class":"read","textContent":"r"}},
								{"div":{"class":"write","textContent":"w"}},
								{"div":{"class":"execute","textContent":"x"}}
							]
						}},
						{"a": {
							"textContent": "{{filename}}",
							"title": "{{pathname}}",
							"href": "{{pathname}}"
						}}
					]
				}},
				{"td": {
					"class":"ownername",
					"title":"{{ownername}}'s uid is {{filestat.uid}}",
					"textContent": "{{ownername}}"
				}},
				{"td": {
					"class":"groupname",
					"title":"{{groupname}}'s uid is {{filestat.gid}}",
					"textContent": "{{groupname}}"
				}},
				{"td": {
					"class":"mimetype",
					"textContent": "{{mimetype}}"
				}},
				{"td": {
					"class":"size",
					"title": "{{filestat.size}} bytes",
					"textContent": "{{readableSize}}"
				}}
			]
		}}
	]
}}