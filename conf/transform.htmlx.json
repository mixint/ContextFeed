{"table": {
	"id": "{{newRandomID}}",
	"class": "file",
	"childNodes": [
		{"tr": {
			"childNodes": [
				{"td":{"class":"mode ur {{permission.ur}}","textContent":"r"}},
				{"td":{"class":"mode gr {{permission.gr}}","textContent":"r"}},
				{"td":{"class":"mode or {{permission.or}}","textContent":"r"}},
				{"td":{
					"class":"filename",
					"rowspan":"3",
					"childNodes": [
						{"a": {
							"textContent": "{{filename}}",
							"title": "{{pathname}}",
							"href": "{{pathname}}"
						}}
					]
				}},
				{"td": {
					"class":"ownername",
					"rowspan":"3",
					"title":"{{ownername}}'s uid is {{filestat.uid}}",
					"textContent": "{{ownername}}"
				}},
				{"td": {
					"class":"groupname",
					"rowspan":"3",
					"title":"{{groupname}}'s uid is {{filestat.gid}}",
					"textContent": "{{groupname}}"
				}},
				{"td": {
					"class":"mimetype",
					"rowspan":"3",
					"textContent": "{{mimetype}}"
				}},
				{"td": {
					"class":"size",
					"rowspan":"3",
					"title": "{{filestat.size}} bytes",
					"textContent": "{{readableSize}}"
				}},
				{"td": {
					"class":"mtime",
					"rowspan":"3",
					"title": "{{filestat.mtime}}",
					"textContent": "{{readableDate}}"
				}}
			]
		}},
		{"tr": {
			"childNodes": [
				{"td":{"class":"mode uw {{permission.uw}}","textContent":"w"}},
				{"td":{"class":"mode gw {{permission.gw}}","textContent":"w"}},
				{"td":{"class":"mode ow {{permission.ow}}","textContent":"w"}}
			]
		}},
		{"tr": {
			"childNodes": [
				{"td":{"class":"mode ux {{permission.ux}}","textContent":"x"}},
				{"td":{"class":"mode gx {{permission.gx}}","textContent":"x"}},
				{"td":{"class":"mode ox {{permission.ox}}","textContent":"x"}}
			]
		}}
	]
}}